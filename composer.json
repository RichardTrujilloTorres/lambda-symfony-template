{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "bref/bref": "^2.4",
        "bref/symfony-bridge": "^1.1",
        "symfony/console": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.3.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "7.3.*",
        "symfony/validator": "7.3.*",
        "symfony/yaml": "7.3.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "start": [
            "APP_ENV=dev symfony server:start"
        ],
        "start-php": [
            "APP_ENV=dev php -S localhost:8000 -t public"
        ],
        "test": "vendor/bin/phpunit",
        "test:coverage": "php -d xdebug.mode=coverage vendor/bin/phpunit --coverage-text",
        "test:coverage-xml": "php -d xdebug.mode=coverage vendor/bin/phpunit --coverage-clover build/logs/clover.xml",
        "cs:fix": "php-cs-fixer fix --config=.php-cs-fixer.php",
        "cs:check": "php-cs-fixer fix --config=.php-cs-fixer.php --dry-run --diff",
        "stan": "phpstan analyse",
        "stan:baseline": "phpstan analyse --generate-baseline=phpstan-baseline.neon",
        "deploy": "serverless deploy",
        "remove": "serverless remove",
        "logs": "serverless logs -f web --tail",
        "cache:clear": [
            "APP_ENV=prod APP_DEBUG=0 php bin/console cache:clear"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "require-dev": {
        "captainhook/captainhook": "^5.25",
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "*",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*"
    }
}
